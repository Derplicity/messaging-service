            "additionalProperties": false,
            "properties": {
              "id": {
                "type": "string",
                "format": "uuid"
              },
              "updateValues": {
                "type": "object",
                "required": ["firstName", "lastName"],
                "additionalProperties": false,
                "properties": {
                  "firstName": {
                    "type": "string"
                  },
                  "lastName": {
                    "type": "string"

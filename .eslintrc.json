{
  "env": {
    "mocha": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "global-require": "off",
    "no-console": "off",
    "no-underscore-dangle": "off"
  },
  "overrides": [
    {
      "files": "*.test.js",
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ]
}
